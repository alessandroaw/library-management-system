  <!-- preloader area start -->
  <div id="preloader">
    <div class="loader"></div>
  </div>
  <!-- preloader area end -->
  <!-- main wrapper start -->
  <div class="horizontal-main-wrapper">
    <!-- main header area start -->
    <%- include('../partials/header-mahasiswa') %>
    <!-- main header area end -->
    <!-- header area start -->
    <div class="header-area header-bottom">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-lg-12  d-none d-lg-block">
            <div class="card text-center">
              <div class="card-header">
                <ul class="nav nav-tabs card-header-tabs">
                  <li class="nav-item col-sm-4">
                    <a class="nav-link" href="/mahasiswa/borrow">Peminjaman</a>
                  </li>
                  <li class="nav-item col-sm-4">
                    <a class="nav-link" href="/mahasiswa/return">Pengembalian</a>
                  </li>
                  <li class="nav-item col-sm-4">
                    <a class="nav-link active" href="/mahasiswa/recommendation">Rekomendasi</a>
                  </li>
                </ul>
              </div>
              <div class="card-body">
                <h5 class="card-title">Rekomendasi Buku</h5>
                <p class="card-text">Silahkan merekomendasikan buku yang perlu diadakan oleh perpustakaan kami.</p>
                <!-- nav and search button -->
                <div class="col-lg-auto clearfix pt-4">
                  <div class="search-box pb-5">
                    <form id="book-form">
                      <input type="text" placeholder="Masukkan judul/penulis" style="text-align:center">
                      <!-- <i class="ti-search"></i> -->
                    </form>
                  </div>
                </div>
                <!-- template rekomendasi -->
                <div class="container" id="search-result" >
                  <script id="template" type="html">
                    <div class="row">
                    {{#books}}
                    {{#.}}
                      <div class="border col-sm-3 " style="padding:5px;">
                        <img src="{{image}}" alt="{{title}}">
                        <div>
                          <a href="#" class="disabled">{{title}}</a>
                          <p>Penulis : {{author}}</p>
                          <button class="btn btn-primary btn-xs" value="{{index}}">Recommend</button>
                        </div>
                      </div>
                      <!-- <div class="col-3 "> -->
                      <!-- </div> -->
                      <!-- <div class="col-3 "> -->
                      <!-- </div> -->
                      {{/.}}
                      {{/books}}
                    </div>
                </script>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- header area end -->
    <!-- mobile_menu -->
    <div class="col-12 d-block d-lg-none">
      <div id="mobile_menu"></div>
    </div>
    <!-- footer area start-->
    <%- include('../partials/footer') %>

    <!-- footer area end-->
  </div>
</body>
      
