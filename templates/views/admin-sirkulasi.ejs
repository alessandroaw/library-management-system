                <!-- table primary start -->
                <div class="mt-5 col-lg-auto">
                  <div class="card">
                    <div class="card-body">
                      <div class="single-table">
                        <div class="table-responsive">


                          <!--template sebelumnya
                          <table class="table text-center">
                          <thead class="text-uppercase bg-primary">
                          <tr class="text-white">
                          <th scope="col">Tgl Pinjam</th>
