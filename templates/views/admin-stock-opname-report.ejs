<!--coba-->
<table id="myTableOpname" class="table table-striped" >
  <thead>
    <tr class="row">
      <th class="col-2 align-self-center">ISBN</th>
      <th class="col-6 align-self-center">Judul</th>
      <th class="col-1 align-self-center">Total</th>
      <th class="col-1 align-self-center">Stok Tersedia</th>
      <th class="col-1 align-self-center">Kondisi Baik</th>
      <th class="col-1 align-self-center">Kondisi Buruk</th>
    </tr>
  </thead>

  <tbody>
    <!--ejs-->
    <% for (var i = 0; i < data.length;  i++) { %>
      <tr class="row">
        <td class="col-2 align-self-center"><%= data[i].isbn %></td>
        <td class="col-6 align-self-center"><%= data[i].title %></td>
        <td class="col-1 align-self-center"><%= data[i].total %></td>
        <td class="col-1 align-self-center"><%= data[i].stock %></td>
        <td class="col-1 align-self-center"><%= data[i].goodCondition %></td>
        <td class="col-1 align-self-center"><%= data[i].badCondition %></td>
      </tr>
      <% } %>
      <!--/ejs-->
    </tbody>
  </table>
